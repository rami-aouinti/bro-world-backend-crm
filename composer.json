{
  "name": "bro-world/crm-microservice",
  "type": "project",
  "description": "JSON REST API",
  "keywords": [
    "Docker",
    "Nginx",
    "Php",
    "Symfony",
    "API",
    "Supervisord",
    "MySQL",
    "RabbitMQ",
    "Elasticsearch"
  ],
  "homepage": "https://github.com/rami-aouinti/bro-world-backend-crm",
  "version": "v1.0.0",
  "license": "proprietary",
  "authors": [
    {
      "name": "Rami Aouinti",
      "email": "rami.aouinti@gmail.com",
      "homepage": "https://github.com/rami-aouinti",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^4.1.19",
    "bro-world/core-bundle": "^0.1.7",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.18.1",
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "doctrine/doctrine-migrations-bundle": "^3.5.0",
    "doctrine/orm": "^2.20.7",
    "fakerphp/faker": "^1.24.1",
    "gesdinet/jwt-refresh-token-bundle": "^1.1",
    "lexik/jwt-authentication-bundle": "^3.1.1",
    "liip/imagine-bundle": "^2.10",
    "nelmio/cors-bundle": "^2.6",
    "oneup/flysystem-bundle": "^4.4",
    "stof/doctrine-extensions-bundle": "^1.7",
    "symfony/asset": "7.2.*",
    "symfony/console": "7.2.*",
    "symfony/dotenv": "7.2.*",
    "symfony/expression-language": "7.2.*",
    "symfony/flex": "^2.9.0",
    "symfony/framework-bundle": "7.2.*",
    "symfony/http-foundation": "7.3.7",
    "symfony/mailer": "7.2.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/runtime": "7.2.*",
    "symfony/security-bundle": "7.2.*",
    "symfony/translation": "7.2.*",
    "symfony/twig-bundle": "7.2.*",
    "symfony/validator": "7.2.*",
    "symfony/yaml": "7.2.*",
    "vich/uploader-bundle": "^2.8"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "platform": {
      "php": "8.3.0"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "prepare": [
      "php bin/console doctrine:database:drop --if-exists --force",
      "php bin/console doctrine:database:create",
      "php bin/console doctrine:schema:update --force --complete",
      "php bin/console doctrine:fixtures:load -n"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.64.0",
    "symfony/var-dumper": "7.2.*",
    "symfony/web-profiler-bundle": "7.2.*"
  },
  "support": {
    "issues": "https://github.com/rami-aouinti/bro-world-backend-crm/issues"
  }
}
